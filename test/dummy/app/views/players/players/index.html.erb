<p id="notice"><%= notice %></p>

<h1>Players Players</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players_players.each do |players_player| %>
      <tr>
        <td><%= players_player.name %></td>
        <td><%= link_to 'Show', players_player_path(players_player) %></td>
        <td><%= link_to 'Edit', edit_players_player_path(players_player) %></td>
        <td><%= link_to 'Destroy', players_player_path(players_player), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Players Player', new_players_player_path %>